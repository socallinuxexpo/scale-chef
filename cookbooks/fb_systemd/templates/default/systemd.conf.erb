[<%= @section %>]
<% if node['fb_systemd'][@config]['config']
     keys = node['fb_systemd'][@config]['config'].to_hash
   else
     keys = node['fb_systemd'][@config].to_hash
   end
   keys.sort.each do |key, val|
     if val.is_a?(TrueClass)
       v = 'yes'
     elsif val.is_a?(FalseClass)
       v = 'no'
     elsif val.is_a?(Array)
       v = val.join(' ')
     else
       v = val
     end %>
<%=  key %>=<%= v %>
<% end %>
