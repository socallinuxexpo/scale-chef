[Unit]
Description=Manage swap file
DefaultDependencies=no
Requires=systemd-remount-fs.service
After=systemd-remount-fs.service
RequiresMountsFor=/usr/local/libexec/manage-swap-file <%= node['fb_swap']['filesystem'] %>

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/local/libexec/manage-swap-file
